    "setup:staging": "bash scripts/setup-environment.sh staging",
    "setup:prod": "bash scripts/setup-environment.sh production",
    "backup": "bash scripts/backup.sh",
    "restore": "bash scripts/restore.sh",
    "health-check": "bash scripts/health-check.sh",
    "env:generate": "node scripts/generate-secrets.js",
    "env:validate": "node scripts/validate-env.js",
    "env:setup": "cp .env.example .env && node scripts/generate-secrets.js"
